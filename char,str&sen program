#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
	char ch;
	char str[10];
	char sen[100];
	printf("Input:\n");
	scanf("%c",&ch);
	scanf("%s",str);
	scanf("\n");
	scanf("%[^\n]%*c",sen);
	printf("Output:\n");
	printf("%c\n",ch);
	printf("%s\n",str);
	printf("%s",sen);
	return 0;
}
