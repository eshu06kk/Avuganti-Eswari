#include<stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
	char str[100];
	gets(str);
	int i,flag=0,n=0;
	n=strlen(str);
	for(i=0;i<n/2;i++)
	{
		if(str[i]!=str[n-1-i])
		{
			flag=1;
			break;
		}
	}
	if(flag==0)
	{
	printf("Palindrome");
	}
	else
	{
		printf("Not Palindrome");
	}
    
}
