#include<stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
	char str[10];
	gets(str);
	int i,j,count1=0,count2=0;
	for(i=0;str[i]!='\0';i++)
	{
			if(isupper(str[i]))
			{
				count2=count2+1;
			}
	}
	for(j=0;str[j]!='\0';j++)
	{
		if(islower(str[j]))
		{
			count1=count1+1;
		}
    }
    
	printf("%d\n",count1);
    printf("%d\n",count2);
}
