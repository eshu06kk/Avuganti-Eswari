int main() {
    int a,b,c,d,max=0;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    if (a>b && a>c)
    {
        max=a;
    }
    else if(b>c && b>a)
    {
        max=b;
    }
    else
    {
        max=c;
    }
    if (max>d)
    {
        printf("%d",max);
    }
    else {
        printf("%d", d);
    }
    return 0;
}
